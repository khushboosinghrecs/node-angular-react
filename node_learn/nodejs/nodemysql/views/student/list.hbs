<h3><a href="/student">create new</a>Student List</h3>
<table class="table-striped">
    <thead>
        <tr>
            <th>Full name</th>
            <th>Email</th>
            <th>mobile</th>
            <th>city</th>
        </tr>
    </thead>
    <tbody>
        {{#each list}}
        <tr>
            <td>{{this.fullName}}</td>
            <td>{{this.email}}</td>
            <td>{{this.mobile}}</td>
            <td>{{this.city}}</td>
            <td>
                <a href="/student/{{this.id}}" class="btn-primary">Edit</a>
                <a href="/student/delete/{{this.id}}" class="btn-primary" onclick="return confirm("Are you want to delete this record?");">Delete</a>
            </td>

        </tr>
        {{/each}}
    </tbody>
</table>